/* component/downloadDialog/dialog.wxss */
.dialog{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 3;
}
.dialog .mask{
  height: 100%;
  width: 100%;
  background: rgba(0, 0, 0, .5);
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.dialog .mask .content{
  width: 630rpx; 
  border-radius: 16rpx;
  background: #fff;
  padding: 40rpx 56rpx;
  box-sizing: border-box;
}
.dialog .mask .closeBtn{
  width: 76rpx;
  height: 76rpx;
  margin-top: 60rpx;
}
.txtInfo{
  font-size: 24rpx;
  text-align: center;
  /*  color:rgb(251, 200, 115); */
}
.logosBox{
  margin-top: 46rpx;    
}
.logoLine{
  display: flex;
  justify-content: space-around;
  margin-top: 40rpx; 
}
.logoLine .logoBox{
  font-size: 24rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.logoLine .logoBox .logoTxt{
  min-width: 158rpx;
  text-align: center;
  margin-top: 10rpx;
}
.logoLine .logoBox .logo{
  width: 56rpx;
  height: 56rpx;
}